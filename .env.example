# Configurações do BigQuery
BIGQUERY_PROJECT_ID=seu-projeto-id
BIGQUERY_KEY_FILE=caminho/para/sua/chave.json
BIGQUERY_LOCATION=US
BIGQUERY_MAX_BYTES=1073741824 # 1GB em bytes

# Configurações do Pinecone
PINECONE_API_KEY=sua-chave-api
PINECONE_ENVIRONMENT=seu-ambiente
PINECONE_INDEX=seu-indice

# Configurações do OpenAI
OPENAI_API_KEY=sua-chave-api
OPENAI_MODEL=gpt-4-turbo-preview

# Configurações do Vertex AI
VERTEX_AI_PROJECT_ID=seu-projeto-id
VERTEX_AI_LOCATION=us-central1
VERTEX_AI_MODEL=text-bison@002

# Clerk Authentication
CLERK_SECRET_KEY=your_secret_key_here
CLERK_PUBLISHABLE_KEY=your_publishable_key_here
CLERK_WEBHOOK_SECRET=your_webhook_secret_here
CLERK_JWT_KEY=your_jwt_key_here

# Clerk Frontend Configuration
NEXT_PUBLIC_CLERK_FRONTEND_API=your_frontend_api_here
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/dashboard
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/onboarding

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_TTL=3600
REDIS_MAX_ITEMS=100

# Bull Queue Configuration
BULL_JOB_ATTEMPTS=3
BULL_BACKOFF_DELAY=1000
BULL_JOB_TIMEOUT=30000
BULL_REMOVE_ON_COMPLETE=true
BULL_REMOVE_ON_FAIL=false
BULL_RATE_LIMIT_MAX=100
BULL_RATE_LIMIT_DURATION=60000

# Application
NODE_ENV=development
PORT=3000
